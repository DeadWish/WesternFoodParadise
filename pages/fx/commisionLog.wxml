<image class="logo" src="/images/yongjin_banner.png" mode="widthFix" />
<view class="weui-cells__title title">{{_t['返佣明细'] || '返佣明细'}}</view>
<view wx:if="{{commisionLog.length <= 0}}" class="weui-loadmore weui-loadmore_line" style='margin-top:200rpx;'>
  <view class="weui-loadmore__tips weui-loadmore__tips_in-line">{{_t['暂无数据'] || '暂无数据'}}</view>
</view>
<view wx:if="{{commisionLog.length > 0}}" class="weui-panel weui-panel_access">
  <view class="weui-panel__bd">
    <view wx:for="{{commisionLog}}" wx:key="*this" class="weui-media-box weui-media-box_text">
      <view class="weui-media-box__title weui-media-box__title_in-text">
        <text style='color:red'>{{ item.money }} {{item.unit==0? (_t['元'] || '元'):(_t['积分'] || '积分')}}</text>

        <view class="weui-badge" style="margin-left: 5px;" wx:if='{{language == "en"}}'>
          Level {{item.level}} commission ratio is {{ item.bili }}%
        </view>

        <view class="weui-badge" style="margin-left: 5px;" wx:else>
          {{item.level}} 级返佣 {{ item.bili }}%
        </view>

        <text wx:if="{{item.isSettlement}}" style='color:green;margin-left:10px;font-size:14px;'>{{_t['已到账'] || '已到账'}}</text>

        <text wx:else style='color:gray;margin-left:10px;font-size:14px;'>{{_t['未到账'] || '未到账'}}</text>

      </view>
      <view class="weui-media-box__desc"  wx:if='{{language == "en"}}'>
      {{ item.nicks }} spent money {{ item.dateAdd }}
      </view>
      <view class="weui-media-box__desc" wx:else>
      {{ item.nicks }} 于 {{ item.dateAdd }} 消费
      </view>
    </view>
  </view>
</view>